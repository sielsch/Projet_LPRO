<?xml version="1.0"?><!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>

	<class name="DateAccesZone" table="DatesAccesZone">
		<composite-id>
			<key-property name="idEmploye" type="int" column="idEmploye"></key-property>
			<key-property name="idZone" type="integer" column="idZone"></key-property>
			<key-property name="dateAcces" type="date" column="dateAcces"></key-property>
		</composite-id>
	</class>


</hibernate-mapping>