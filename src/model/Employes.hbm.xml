<?xml version="1.0"?><!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd"><hibernate-mapping>
  <class name="Employes" table="Employes">
    <id name="idEmploye" type="integer" column="IdEmploye">
      <generator class="native"/>
    </id>
    <property name="nom" type="string" column="nom" not-null="true" />
    <property name="prenom" type="string" column="prenom" not-null="true" />
    <property name="password" type="string" column="password" not-null="true" />
    <property name="idFonction" type="integer" column="idFonction" not-null="true" />
  </class>

  
</hibernate-mapping>